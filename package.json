{
  "name": "@manuscripts/comment-editor",
  "description": "React components for editing and viewing manuscript comments",
  "version": "0.2.3",
  "repository": "github:Atypon-OpenSource/manuscripts-comment-editor.git",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "prosemirror"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "dev": "npm-run-all --parallel 'build:* --watch'",
    "lint": "eslint src --ext .ts,.tsx",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "dependencies": {
    "react-select": "~3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@manuscripts/eslint-config": "^0.5.1",
    "@manuscripts/publish": "^0.2.2",
    "@types/jest": "^29.2.4",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "~3.0.21",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prosemirror-commands": "~1.5.0",
    "prosemirror-history": "~1.3.0",
    "prosemirror-inputrules": "~1.2.0",
    "prosemirror-keymap": "~1.2.0",
    "prosemirror-model": "~1.18.3",
    "prosemirror-test-builder": "^1.1.0",
    "prosemirror-transform": "~1.7.0",
    "prosemirror-state": "~1.4.2",
    "prosemirror-view": "~1.29.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-dom": ">=16.11.0",
    "prosemirror-commands": "~1.5.0",
    "prosemirror-history": "~1.3.0",
    "prosemirror-inputrules": "~1.2.0",
    "prosemirror-keymap": "~1.2.0",
    "prosemirror-model": "~1.18.3",
    "prosemirror-transform": "~1.7.0",
    "prosemirror-state": "~1.4.2",
