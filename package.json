{
  "name": "@manuscripts/comment-editor",
  "description": "React components for editing and viewing manuscript comments",
  "version": "0.1.0",
  "repository": "https://gitlab.com/mpapp-private/manuscripts-comment-editor",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "prosemirror"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "dev": "tsc --outDir dist/es --watch",
    "lint": "tslint --project tsconfig.json",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"{src}/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "publishConfig": {
    "@manuscripts:registry": "https://atypon-npm-registry.herokuapp.com/"
  },
  "dependencies": {
    "prosemirror-commands": "^1.0.4",
    "prosemirror-history": "^1.0.0",
    "prosemirror-inputrules": "^1.0.1",
    "prosemirror-keymap": "^1.0.0",
    "prosemirror-model": "^1.6.2",
    "prosemirror-state": "^1.2.2",
    "prosemirror-view": "^1.6.3",
    "react-select": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.7",
    "@types/prosemirror-commands": "^1.0.0",
    "@types/prosemirror-history": "^1.0.0",
    "@types/prosemirror-inputrules": "^1.0.2",
    "@types/prosemirror-keymap": "^1.0.0",
    "@types/prosemirror-model": "^1.1.1",
    "@types/prosemirror-state": "^1.0.0",
    "@types/prosemirror-view": "^1.3.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-select": "^2.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.10.2",
    "prosemirror-test-builder": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.4.0",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
