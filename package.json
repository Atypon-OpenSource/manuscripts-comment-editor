{
  "name": "@manuscripts/comment-editor",
  "description": "React components for editing and viewing manuscript comments",
  "version": "0.2.0",
  "repository": "gitlab:mpapp-public/manuscripts-comment-editor",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "prosemirror"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "dev": "npm-run-all --parallel 'build:* --watch'",
    "lint": "tslint --project .",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preversion": "npm-run-all --parallel lint typecheck test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "dependencies": {
    "prosemirror-commands": "^1.1.3",
    "prosemirror-history": "^1.1.3",
    "prosemirror-inputrules": "^1.1.2",
    "prosemirror-keymap": "^1.1.3",
    "prosemirror-model": "^1.9.1",
    "prosemirror-state": "^1.3.2",
    "prosemirror-view": "^1.14.2",
    "react-select": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@manuscripts/publish": "^0.2.2",
    "@manuscripts/tslint-config": "^0.1.4",
    "@types/jest": "^25.1.3",
    "@types/prosemirror-commands": "^1.0.1",
    "@types/prosemirror-history": "^1.0.1",
    "@types/prosemirror-inputrules": "^1.0.2",
    "@types/prosemirror-keymap": "^1.0.1",
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-state": "^1.2.3",
    "@types/prosemirror-view": "^1.11.2",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-select": "^2.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "husky": "^1.1.2",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prosemirror-test-builder": "^1.0.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
